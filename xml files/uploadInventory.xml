<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
<listeners>
	<listener class-name="Listener.MyTestResult"/>
</listeners>
	<test name="Open DealerLogin">
		<classes>
			<class name="Backend_Testcases.DealerLogin"/>
		</classes>
	</test>

	<test name="Upload Inventory">
		<classes>
			<class name="Backend_Testcases.InventoryManagement">
				<methods>
					<include name="checkInventory"></include>
				</methods>
			</class>
		</classes>
	</test>
</suite> 